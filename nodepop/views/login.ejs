                      <div class="invalid-feedback" data-sb-feedback="email:email">Email no v치lido.</div>
                  </div>
                  <div class="form-group">
                    <!-- Name input-->
                    <input class="form-control" id="password" type="password" placeholder="Tu Contrase침a *" data-sb-validations="required" />
                    <div class="invalid-feedback" data-sb-feedback="password:required">Contrase침a obligatoria.</div>
                </div>
          </div>
          <!-- Submit Button-->
          <div class="text-center"><button class="btn btn-primary btn-xl text-uppercase disabled" id="submitButton" type="submit">Acceder</button></div>
      </form>
  </div>
</section>

